{
  "name": "react-oidc-context",
  "version": "1.0.3",
  "description": "OpenID Connect & OAuth2 authentication using react context api as state management",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:pamapa/react-oidc-context.git"
  },
  "homepage": "https://github.com/pamapa/react-oidc-context#readme",
  "license": "MIT",
  "main": "dist/react-oidc-context.cjs",
  "module": "dist/react-oidc-context.mjs",
  "types": "dist/react-oidc-context.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "authentication",
    "context-api",
    "oauth2",
    "oidc",
    "openid",
    "OpenID Connect",
    "react-component"
  ],
  "scripts": {
    "start": "tsc --noEmit --watch",
    "build": "npm run build-esm && npm run build-node && npm run build-types",
    "build-esm": "esbuild src/index.ts --bundle --outfile=dist/react-oidc-context.mjs --platform=neutral --external:react --external:oidc-client-ts",
    "build-node": "esbuild src/index.ts --bundle --outfile=dist/react-oidc-context.cjs --platform=node",
    "build-types": "tsc --emitDeclarationOnly && api-extractor run",
    "prepack": "npm run build",
    "test": "tsc -p test/tsconfig.json && jest",
    "lint": "eslint ./src/**/*.ts"
  },
  "peerDependencies": {
    "oidc-client-ts": "^2.0.0-0",
    "react": ">=16"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.11",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "esbuild": "^0.13.2",
    "eslint": "^7.32.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "jest": "^27.2.1",
    "oidc-client-ts": "^2.0.0-alpha2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "~4.4.3"
  },
  "jest": {
    "clearMocks": true,
    "testURL": "https://www.example.com/",
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=10"
  }
}
